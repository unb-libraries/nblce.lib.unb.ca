{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true,
      "tbachert/spi": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/ctools": "^4.0.0",
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of nblce.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {}
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/nblce.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip"
        },
        "name": "dinbror/blazy",
        "type": "drupal-library",
        "version": "1.8.2"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/unb-libraries/upstream-library-mirror/raw/master/link/link_4.20.1.zip"
        },
        "name": "ckeditor/link",
        "type": "drupal-library",
        "version": "4.20.1"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/kenwheeler/slick/archive/refs/tags/1.8.0.zip"
        },
        "name": "kenwheeler/slick",
        "type": "drupal-library",
        "version": "1.8.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/markgoodyear/scrollup/archive/refs/heads/master.zip"
        },
        "extra": {
          "installer-name": "scrollup"
        },
        "name": "markgoodyear/scrollup",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    }
  ],
  "require": {
    "ckeditor/link": "^4",
    "dinbror/blazy": "^1",
    "drupal/admin_toolbar": "^3",
    "drupal/asset_injector": "^2",
    "drupal/blazy": "^3",
    "drupal/bootstrap4": "^3",
    "drupal/ckeditor": "^1",
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^2",
    "drupal/jquery_scrollup": "^2",
    "drupal/login_destination": "^2",
    "drupal/menu_entity_index": "^1",
    "drupal/metatag": "^1",
    "drupal/node_title_help_text": "^1",
    "drupal/noreqnewpass": "^1",
    "drupal/pathauto": "^1",
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "<1.35",
    "drupal/search_api_solr": "^4",
    "drupal/simple_sitemap": "^4",
    "drupal/slick": "^3",
    "drupal/slick_views": "^3",
    "drupal/token": "^1",
    "kenwheeler/slick": "<1.8.1",
    "markgoodyear/scrollup": "dev-master",
    "unb-libraries/saml_features": "dev-10.x-1.x",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
