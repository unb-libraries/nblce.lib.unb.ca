{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of nblce.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/samlauth": {
                "Ignore Role Restrictions": "/build/patches/samlauth_rolematch.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/nblce.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/fakeobjects",
                "version": "4.16.0",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/unb-libraries/upstream-library-mirror/raw/master/fakeobjects/fakeobjects_4.16.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/link",
                "version": "4.16.0",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/unb-libraries/upstream-library-mirror/raw/master/link/link_4.16.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/kenwheeler/slick/archive/refs/tags/v1.8.1.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "markgoodyear/scrollup",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/markgoodyear/scrollup/archive/refs/heads/master.zip"
                },
                "extra": {
                    "installer-name": "scrollup"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "w8tcha/ckeditor-codemirror-plugin",
                "version": "1.17.12",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/w8tcha/CKEditor-CodeMirror-Plugin/archive/v1.17.12.zip"
                },
                "extra": {
                    "installer-name": "ckeditor_codemirror"
                }
            }
        }
    ],
    "require": {
        "ckeditor/fakeobjects": "~4.16",
        "ckeditor/link": "~4.16",
        "dinbror/blazy": "^1.8.0",
        "drupal/admin_toolbar": "3.1.0",
        "drupal/anchor_link": "2.5",
        "drupal/asset_injector": "2.8",
        "drupal/bootstrap4": "2.1.14",
        "drupal/ckeditor_codemirror": "2.3",
        "drupal/core": "9.3.3",
        "drupal/fakeobjects": "1.1",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.3",
        "drupal/jquery_scrollup": "1.0",
        "drupal/login_destination": "2.0-beta1",
        "drupal/metatag": "1.19",
        "drupal/menu_entity_index": "1.3",
        "drupal/node_title_help_text": "1.2",
        "drupal/noreqnewpass": "1.2",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.5",
        "drupal/samlauth": "3.4",
        "drupal/search_api": "1.23",
        "drupal/search_api_solr": "4.2.6",
        "drupal/simple_sitemap": "3.11",
        "drupal/slick": "2.4",
        "drupal/slick_views": "2.6",
        "drupal/token": "1.10",
        "drupal/twig_tweak": "2.9",
        "kenwheeler/slick": "^1.8",
        "markgoodyear/scrollup": "dev-master",
        "unb-libraries/drupal-builder": "^9",
        "unb-libraries/saml_features": "dev-8.x-1.x",
        "w8tcha/ckeditor-codemirror-plugin": "^1.17.12"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^9"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
